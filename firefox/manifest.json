{
	"name": "TwitchAdBlock",
	"version": "1.0",
	"description": "Blocks twitch ads.",
	"background": {
		"scripts": ["redirect.js"]
	},
	"web_accessible_resources": [
		"redirect.js"
	],
	"manifest_version": 2,	
	"permissions": ["webRequest", "webRequestBlocking", "*://*.twitch.tv/*"],
	"applications": {
		"gecko": {
			"id": "@mozilla.org"
		}
	}
}